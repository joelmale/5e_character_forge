{
  "name": "5e-character-forge",
  "version": "1.0.0",
  "description": "A client-side D&D 5e character generator and sheet manager",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "size-limit": "size-limit",
    "docker:build-push": "bash ./scripts/docker-build-push.sh"
  },
  "dependencies": {
    "@3d-dice/dice-box": "^1.1.4",
    "@3d-dice/dice-parser-interface": "^0.2.1",
    "@3d-dice/dice-ui": "^0.5.2",
    "lucide-react": "^0.293.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@size-limit/preset-app": "^11.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.49",
    "size-limit": "^11.2.0",
    "tailwindcss": "^3.4.15",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vitest": "^4.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joelmale/5e_character_forge.git"
  },
  "keywords": [
    "dnd",
    "dungeons-and-dragons",
    "character-sheet",
    "react",
    "typescript",
    "indexeddb"
  ],
  "author": "Joel Male",
  "license": "MIT"
