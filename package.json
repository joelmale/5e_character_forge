{
  "name": "5e-character-forge",
  "version": "1.5.3",
  "description": "A client-side D&D 5e character generator and sheet manager",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "size-limit": "size-limit",
    "docker:build": "bash ./scripts/fast-build.sh",
    "docker:build-push": "bash ./scripts/docker-build-push.sh",
    "docker:run": "docker stop 5e-forge 2>/dev/null || true && docker rm 5e-forge 2>/dev/null || true && docker run -d --name 5e-forge -p 8080:80 joelmale/5e-character-forge:latest && echo '\u2705 Container started at http://localhost:8080'",
    "docker:stop": "docker stop 5e-forge && docker rm 5e-forge && echo '\u2705 Container stopped and removed'",
    "docker:logs": "docker logs -f 5e-forge",
    "docker:restart": "docker restart 5e-forge && echo '\u2705 Container restarted'"
  },
  "dependencies": {
    "@3d-dice/dice-box": "^1.1.4",
    "@3d-dice/dice-parser-interface": "^0.2.1",
    "@3d-dice/dice-ui": "^0.5.2",
    "@tiptap/extension-underline": "^3.14.0",
    "@tiptap/pm": "^3.14.0",
    "@tiptap/react": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.561.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@size-limit/preset-app": "^12.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.49",
    "size-limit": "^12.0.0",
    "tailwindcss": "^4.1.17",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joelmale/5e_character_forge.git"
  },
  "keywords": [
    "dnd",
    "dungeons-and-dragons",
    "character-sheet",
    "react",
    "typescript",
    "indexeddb"
  ],
  "author": "Joel Male",
  "license": "MIT"
}
